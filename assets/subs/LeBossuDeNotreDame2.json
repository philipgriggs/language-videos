{
    "data": [
        {
            "start": 0.5,
            "end": 1.779999999999987,
            "str": "Mon sanctuaire..."
        },
        {
            "start": 3.1799999999999926,
            "end": 6.719999999999999,
            "str": "À l'abri <des fenêtres> et des parapets de pierre"
        },
        {
            "start": 6.719999999999999,
            "end": 10.14,
            "str": "Je regarde vivre les gens <d'en bas>"
        },
        {
            "start": 11.079999999999998,
            "end": 14.439999999999998,
            "str": "<Chaque jour> j'envie leur vie moi qui vis solitaire"
        },
        {
            "start": 14.579999999999998,
            "end": 18.33999999999999,
            "str": "Mais leur histoire je ne la <connais pas>"
        },
        {
            "start": 18.86,
            "end": 22.239999999999995,
            "str": "J'apprends leurs <chansons>, leurs <rires>, leurs <visages>"
        },
        {
            "start": 22.679999999999993,
            "end": 26.040000000000006,
            "str": "Moi je les vois mais eux ne me <voient pas>"
        },
        {
            "start": 26.72,
            "end": 30.379999999999995,
            "str": "Je voudrais, tour à tour, rencontrer <ces personnages>"
        },
        {
            "start": 30.42,
            "end": 38.11999999999999,
            "str": "<Rien qu'un seul jour>, au pied des tours"
        },
        {
            "start": 38.11999999999999,
            "end": 44.239999999999995,
            "str": "<Tout en bas>, vivre au grand <soleil>"
        },
        {
            "start": 44.239999999999995,
            "end": 51.27999999999999,
            "str": "Sans regarder <le ciel>, une seule <fois>"
        },
        {
            "start": 51.27999999999999,
            "end": 53.739999999999995,
            "str": "Partager <leur joie>"
        },
        {
            "start": 53.739999999999995,
            "end": 58.959999999999994,
            "str": "Je crois, <qu'ils n'entendent pas>"
        },
        {
            "start": 58.999999999999986,
            "end": 62.499999999999986,
            "str": "La voix <de mon coeur>"
        },
        {
            "start": 64.16000000000001,
            "end": 66.93999999999998,
            "str": "Qui se <meurt>"
        },
        {
            "start": 67.48,
            "end": 77.0,
            "str": "<Quand je vois> les gens d'en bas!"
        },
        {
            "start": 78.26,
            "end": 82.08,
            "str": "En bas <j'entends> les tisserands, les meuniers et leurs femmes"
        },
        {
            "start": 82.08,
            "end": 85.37999999999998,
            "str": "Leur bonheur insouciant me <brûle> et m'enflamme"
        },
        {
            "start": 85.67999999999999,
            "end": 89.08,
            "str": "Leurs cris qui résonnent <jusqu'au>  coeur de Notre-Dame"
        },
        {
            "start": 89.08,
            "end": 92.16000000000001,
            "str": "<Font> saigner les larmes au coeur de mon âme."
        },
        {
            "start": 92.39999999999999,
            "end": 96.02,
            "str": "<Si j'avais> cette vie"
        },
        {
            "start": 96.04,
            "end": 101.42,
            "str": "<Je vivrais> à la folie!"
        },
        {
            "start": 101.54,
            "end": 106.86,
            "str": "En bas, sur les bords <de Seine>"
        },
        {
            "start": 107.04,
            "end": 110.26,
            "str": "Je goûterais <la joie>"
        },
        {
            "start": 110.26,
            "end": 115.3,
            "str": "Des <gens> qui se promènent, si <pour un jour>"
        },
        {
            "start": 115.3,
            "end": 121.04,
            "str": "<un seul jour>, je quittais ma tour"
        },
        {
            "start": 121.11999999999999,
            "end": 129.64,
            "str": "Ce serait <merveilleux>, d'être heureux!"
        },
        {
            "start": 129.76,
            "end": 136.39999999999998,
            "str": "À mon tour, <faire un tour>, alentours de ma tour"
        },
        {
            "start": 136.39999999999998,
            "end": 143.01999999999998,
            "str": "<rien qu'un jour>, un jour en"
        },
        {
            "start": 143.01999999999998,
            "end": 146,
            "str": "bas!"
        }
    ]
}